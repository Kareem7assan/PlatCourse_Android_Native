<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:motionInterpolator="@string/material_motion_easing_linear"
        motion:constraintSetStart="@id/start"
        motion:duration="10000">
       <KeyFrameSet>

           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="0"
               motion:keyPositionType="parentRelative"
               motion:percentX="0"
               motion:percentY="0"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="5"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.1"
               motion:percentY="0.1"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="10"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.2"
               motion:percentY="0.2"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="15"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.3"
               motion:percentY="0.3"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="20"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.4"
               motion:percentY="0.4"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="25"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.5"
               motion:percentY="0.5"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="30"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.4"
               motion:percentY="0.6"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="35"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.3"
               motion:percentY="0.7"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="40"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.2"
               motion:percentY="0.8"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="45"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.1"
               motion:percentY="0.9"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="50"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.1"
               motion:percentY="1"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="55"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.2"
               motion:percentY="0.9"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="60"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.3"
               motion:percentY="0.8"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="65"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.4"
               motion:percentY="0.7"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="70"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.5"
               motion:percentY="0.6"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="75"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.6"
               motion:percentY="0.5"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="80"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.7"
               motion:percentY="0.4"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="85"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.8"
               motion:percentY="0.3"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="90"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.9"
               motion:percentY="0.2"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="95"
               motion:keyPositionType="parentRelative"
               motion:percentX="1.0"
               motion:percentY="0.0"
               motion:drawPath="path" />
           <KeyPosition
               motion:motionTarget="@+id/markLay"
               motion:framePosition="100"
               motion:keyPositionType="parentRelative"
               motion:percentX="1.0"
               motion:percentY="0"
               motion:drawPath="path" />





       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/styledVideo"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="300dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:transitionEasing="cubic(0.2,0.2,1,0.3)" />
        <Constraint
            android:id="@+id/markLay"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            motion:layout_constraintTop_toTopOf="parent"

            />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/markLay"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:visibility="visible"
            />

    </ConstraintSet>
</MotionScene>